{
  "name": "Nandom",
  "version": "1.0",
  "description": "Watch a random netflix movie",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["https://nandom.ew.r.appspot.com/"],
  "browser_action": {
     "default_title": "Nandom" // The title that shows up when a user hovers on your icon
 },
  "icons": {
    "16": "images/Nandom16.png",
    "32": "images/Nandom32.png",
    "48": "images/Nandom48.png",
    "128": "images/Nandom128.png"
  },
  "manifest_version": 2
}